 
 /* setting */
/* 초기화 */
html {overflow-y:scroll}
body {margin:0;padding:0;font-size:0.75em; font-family:'Nanum Gothic', 'Malgun Gothic', dotum, sans-serif; background:#f5f5f5;}
html, h1, h2, h3, h4, h5, h6, form, fieldset, img {margin:0;padding:0;border:0}
h1, h2, h3, h4, h5, h6 {font-size:24px; font-family:'NanumSquare','Malgun Gothic', dotum, sans-serif}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {display:block}

ul, dl,dt,dd {margin:0;padding:0;list-style:none}
legend {position:absolute;margin:0;padding:0;font-size:0;line-height:0;text-indent:-9999em;overflow:hidden}
label, input, button, select, img {vertical-align:middle;font-size:1em}
input, button {margin:0;padding:0; font-family:'Nanum Gothic', 'Malgun Gothic', dotum, sans-serif;font-size:1em}
input[type="submit"]{cursor:pointer}
button {cursor:pointer}

textarea, select {font-family:'Nanum Gothic', 'Malgun Gothic', dotum, sans-serif;font-size:1em}
select {margin:0}
p {margin:0;padding:0;word-break:break-all}
hr {display:none}
pre {overflow-x:scroll;font-size:1.1em}
a {color:#000;text-decoration:none}

img{max-width: 100%; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; }


*, :after, :before {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}


 .header > div{overflow:hidden; max-width:1200px; margin: 0 auto; }
 .header > div > div.logo{}
 .header > div > div.top-btn{}

 /* 헤더 - logo*/
 .header{background:black; overflow: hidden;}
 .header .logo{float:left;}
 .header .logo h1{font-size:17px;}
 .header .logo{display: block; padding: 5px;}
 .header .logo h1 img{width:70px;}

 /* 헤더 - top-btn*/
 .header .top-btn{float:right;}
 .header .top-btn ul{overflow:hidden; margin-top:30px;}
 .header .top-btn ul li{float:left; margin-left:10px;}
 .header .top-btn ul li a{font-size:14px; color:#fff; display: block; padding: 5px;}
/*메인 메뉴*/
 .menu{border-bottom: 1px solid #ccc; background: black; }
 .menu > div{max-width:1200px; margin: 0 auto;}
 .menu > div > ul{text-align:center; border-top: 2px solid #d77300; }
 /*한줄로 만들기*/
 .menu > div > ul > li{display: inline-block; position: relative;} /*이 li가 submenu의 absoulte의 기준이된다. 중앙 정렬로 만들게하기위해서 */
 /*WIDTH:*/
 .menu > div > ul > li a{display: block; padding:10px; font-size:17px; margin:10px 40px; color: white;}
 .menu > div > ul > li a:hover{color:#d77300}
 /*서브 메뉴*/
 .menu > div > ul > li > ul.submenu{position: absolute; z-index: 9; background: white; display: none; width: 100%; border: 1px solid #ccc;} /*absolute는 공중에 띄우는 것처럼 보이지만 항상 기준이 있어야한다. z-index 화면 보여주는 레벨 순서.*/
 .menu > div > ul > li > ul.submenu > li{display: block; border-bottom: 1px solid black; }
 .menu > div ul > li:hover ul.submenu{display: block;}
 .menu > div ul > li ul.submenu a{margin: 5px; font-size: 14px; color: black;} /*margin값으로 넓힘*/
 .menu > div ul > li ul.submenu a:hover{color:#d77300}

 /*배너*/
 .banner .swiper-container{max-width:1200px; margin:0 auto;}
 .banner .swiper-container .swiper-wrapper .swiper-slide img{width:100%;}

 /*공지사항, 우측 배너*/
 .tap-menu ul{overflow:hidden; text-align:center;}
 .tap-menu ul li{float:left; width:33.33%; background: black;}
 .tap-menu ul li a{display:block; padding:5px 10px; border-bottom: 1px solid #e1e1e1; font-size:14px; color: #888;}
 .tap-menu ul li.on a{color:white; font-weight:bold; border-bottom: 2px solid #d77300;}
 .tap-menu ul li:hover a{color:white; }

 .tab-con{}
 .tab-con ul li{overflow:hidden;}
 .tab-con ul li:hover{border-bottom: 1px dashed #e1e1e1;}
 .tab-con ul li a{float:left; display:block; padding:5px; font-size:15px; }
 .tab-con ul li a:befor{content: ""; display: inline-block; width:3px; height: 3px; background: -webkit-border-radius: 50%; -moz-border-radius: 50%;
 vertical-align: middle; margin-right: 5px;}  /*webkit은 크롬, -moz는 사파리*/
 .tab-con ul li span{float:right; padding:5px; font-size:13px; color:#888;}

 .bbs-box > div{overflow:hidden; max-width:1200px; margin:0 auto;}
 .bbs-box > div > .board-box{float:left; width:50%;}
 .bbs-box > div > .board-box > div{padding:10px;}
 .bbs-box > div > .rigth-banner{float:right; width:50%; }
 .bbs-box > div > .rigth-banner .r-slide{padding:10px;}
 .bbs-box > div > .rigth-banner .r-slide .swiper-wrapper .swiper-slide img{height: 200px;}

 /*하단정보*/
 .footer{background: #444; color: #ccc;}
 .footer > div {padding: 20px;}
 .footer h1{font-size: 17px; margin-bottom: 10px;}
 .footer p{font-size:13px; line-height: 160%; padding: 0; margin: 0;}
 .footer span{font-size:12px; color: #999;}


/*로그인*/
 .login-box{padding: 40px;}
 .login-box h1{text-align: center; margin-bottom: 20px;}
 .login-box .login {max-width: 400px; margin: 0 auto; border: 1px solid #e1e1e1; border-radius: 25px; padding: 20px; background: #fff; box-shadow: 1px 6px 11px 3px #cccccc;}
 .login-box .login li {margin: 5px;}
 .login-box .login li input {width: 100%; height: 40px; padding: 5px; border: 1px solid #ccc; border-radius:15px;}
 .login-box .login li button{width: 100%; height: 40px; background: black; color: white; border: 0; border-radius:15px; transition: all 0.3s;} /*transition 애니메이션 효과 자연스럽게하기위해 0.3초동안 변경한다*/
 .login-box .login li button:hover{background: #e1e1e1; border-radius:0; font-size: 14px;}
 
 .login-box .login-btn {text-align: center; margin-top: 20px; transition: all 0.3s;}
 .login-box .login-btn:hover {color: black; font-size: 14px;}

/*회원가입*/
 .join-box{padding: 50px;}
 .join-box .join-form label{display: block;}
 .join-box h1{text-align: center; margin-bottom: 20px;}

 .join-box .join-form{max-width: 400px; margin: 0 auto; border: 1px solid #e1e1e1; border-radius: 25px; padding: 20px; background: #fff; box-shadow: 1px 6px 11px 3px #cccccc;}
 
 /*width 100%해서 가로로 영역 가득 채우기, 안에 글 쓸 때 떨어져보이게 하기 위해서 padding 5px 정도 줌*/
 /*radio 박스가 속성을 벗어나게끔 input[type=] 지정*/
 .join-box .join-form input[type="text"]{width: 100%; height: 40px; padding: 5px; border: 1px solid #ccc; border-radius:15px; margin-bottom: 10px;}
 .join-box .join-form input[type="password"]{width: 100%; height: 40px; padding: 5px; border: 1px solid #ccc; border-radius:15px; margin-bottom: 10px;}
 .join-box .join-form ul li label{font-size: 12px; color: #000; margin-bottom: 8px;}
 .join-box .join-form  ul li {margin: 20px;}

 /*radio 박스 한줄로*/
 .join-box .join-form ul li.gender span{display: inline-block;}
 .join-box .join-form ul li.gender span label{display: inline-block;}

 /*select 박스*/
 .join-box .join-form select{width: 100%; height: 40px; padding: 5px; border: 1px solid #ccc; border-radius:15px; margin-bottom: 10px;}

 /*주소*/
 .join-box .join-form ul li.add span.zipcode input {width:60%;}
 .join-box .join-form ul li.add span.zipcode button {width: 30%; height: 40px; background: black; color: white; border: 0; border-radius:15px; margin-left: 10%; margin-bottom: 10px;}
 .join-box .join-form ul li.add span{margin-bottom: 5px; display: block;}

 .join-box .join-btn {text-align: center; margin-top: 20px;}
 .join-box .join-btn a{display: inline-block; background: #000; color: #fff; margin: 5px; padding: 10px 20px; border-radius:15px;}

/* 공지사항 */
 .contents{max-width: 1200px; margin: 0 auto;}
 .notice-box h1{text-align: center; margin: 20px 0;}
 .search-box {text-align: right; margin: 10px 0;} /*box 자체 정렬 float:right; 요소들을 정리할 때는 text-align*/
 .search-box input{width:100px; border: 1px solid #ccc; height: 30px;}
 .search-box button{border: 1px solid #000; height: 30px; padding: 0 20px; background: #000; color: #fff;}
 .notice-table{background: #fff;}
 .notice-table ul.table-hd{text-align: center; border-top: 2px solid #000;}
 .notice-table ul.table-bd{text-align: center;}
 .notice-table ul.table-bd .subject{text-align: left;} 

 .notice-table ul li{display: table; width: 100%;}
 .notice-table ul li > div{display: table-cell; border-bottom: 1px solid #ccc; padding: 10px;} /*li 바로 밑에 있는 div에게만 해당한다는 의미 >없으면 모든 div에 해당*/
 /*1. class 이름으로 주기*/
 .notice-table ul li > div.no{width: 10%;}
 .notice-table ul li > div.subject{width: 50%;}
 .notice-table ul.table-bd .subject a:hover{text-decoration: underline; color: #d77300;}
 .notice-table ul li > div.name{width: 20%;}
 .notice-table ul li > div.data{width: 20%;}
 /*2. class 안주고 사용하는 법. 단점은 이름 순서를 바꾸었을 때*/
 /*
 .notice-table ul li > div:nth-child(1){width: 10%;}
 .notice-table ul li > div:nth-child(2){width: 50%;}
 .notice-table ul li > div:nth-child(3){width: 20%;}
 .notice-table ul li > div:nth-child(4){width: 20%;}
 */	
 .paging{margin: 20px 0;}
 .paging ul{text-align: center;}
 .paging ul li{display: inline-block; margin: 5px 0px;}
 .paging ul li a{display: block; padding: 5px 10px; border: 1px solid black; color:white; background: black;}
 .paging ul li.on a{background: #fff; color: black; border: 1px solid #000;}

 .bbs-btn-box{text-align: right; margin: 10px 0;}
 .bbs-btn-box a{display: inline-block; background: #000; color: #fff; padding: 5px 10px;}


