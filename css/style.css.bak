/*게시판 뷰*/
.view h2{margin-bottom: 10px;}
.view > span {display: inline-block; margin-bottom: 10px;}

.view .view-con{border-top: 1px solid #ccc; border-bottom: 1px solid #ccc; padding: 20px; background: #fff;}
.view .file-down a{display: block; margin: 5px; padding: 5px; background: #e1e1e1;}

.bbs-btn-box a.del{background: #d77300;}
.bbs-btn-box a.mod{background: #d77300;}

/*갤러리 리스트*/
 .gallery-table > ul{overflow: hidden;}
 .gallery-table > ul > li{width: 25%; float:left; margin-bottom: 20px;} /* width 비율에 맞춰 갯수 조절 3개 33.3%*/
 .gallery-table > ul > li:nth-child(4n+1){clear:both;} /*4번째 다음에 나오는 것 (4의 배수+1) 취소: 왼쪽, 오른쪽을 취소*/
 .gallery-table > ul > li > div{padding: 10px;}
 .gallery-table .thum{ padding-bottom: 0; height: 280px; overflow:hidden;} /*크기를 벗어나는 이미지는 자르기*/
 .gallery-table .info a{display: block; font-size: 15px;}
 .gallery-table .info span{color: #888;}